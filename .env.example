# MongoDB Connection Options
# Option 1: MongoDB Atlas URI (recommended for production)
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-url>/<dbname>?retryWrites=true&w=majority&appName=<app-name>

# Option 2: Self-hosted MongoDB
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_DB_NAME=crop_yield_db
MONGODB_USER=
MONGODB_PASSWORD=

